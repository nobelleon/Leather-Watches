{
	"builds": [{
	    "src": "LEATHERWATCHES/wsgi.py",
	    "use": "@vercel/python",
	    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
	}],
	"routes": [
        { 
        "src": "/assets/(.*)", 
        "dest": "/html/digitalize/assets/$1",
        "headers": {
        "cache-control": "public, max-age=31536000, immutable"
            }
        },
	    {
		"src": "/(.*)",
		"dest": "leatherwatchesapp/templates/index.html"
	    }
	 ]
} 